-- Cập nhật thời gian còn lại
task.spawn(function()
    while task.wait(1) do
        if KeyStartTime ~= nil then
            local elapsed = os.time() - KeyStartTime
            local remain = math.max(KeyDuration - elapsed, 0)
            local h = math.floor(remain / 3600)
            local m = math.floor((remain % 3600) / 60)
            local s = remain % 60
            TimeLabel:Set(string.format(PLcKIDKCoOWucbwIYFqNilHWvQiYDdQXNYUyTtSnatZnOoJIkTvJqwhhYGrctOYprbUIfdcgibHgDuhcxEHGQOREQEKzEnjgs({8820,10920,23625,19635,16485,11025,3360,10815,11025,10185,11550,3360,10395,20475,18690,11550,3360,11340,23625,19530,16905,11025,6090,3360,3885,5040,5250,10500,6090,3885,5040,5250,10500,6090,3885,5040,5250,10500}), h, m, s))
        end
    end
end)

